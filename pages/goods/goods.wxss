.service-policy{
    width: 750rpx;
    height: 73rpx;
    background: #f4f4f4;
    padding: 0 31.25rpx;
    display: flex;
    flex-flow: row nowrap;
    align-items: center;
    justify-content: space-between;
}

.service-policy .item{
    background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/servicePolicyRed-518d32d74b.png) 0 center no-repeat;
    background-size: 10rpx;
    padding-left: 15rpx;
    display: flex;
    align-items: center;
    font-size: 25rpx;
    color: #666;
}

.goods-info{
    width: 750rpx;
    background: white;
    display: flex;
    overflow: visible;
    flex-direction: column;
    z-index: 1;
    position: relative;
}

.goods-info .c{
    display: block;
    height: 100%;
    padding: 38rpx 36rpx;
    border-bottom: 1px solid #f4f4f4;
}

.goods-info .c text{
    display: block;
}

.goods-info .name{
    height: auto;
    margin-bottom: 5.208rpx;
    font-size: 41rpx;
    line-height: 51rpx;
}

.goods-info .desc{
    height: auto;
    font-size: 24rpx;
    color: #999;
}

.goods-info .price-wrapper {
    display: flex;
    flex-direction: row;
    align-items: flex-end;
}
.goods-info .price{
    font-weight: bold;
    font-size: 40rpx;
    /* margin-top: 20rpx; */
}
.goods-info .market-price{
    flex: 1;
    text-decoration: line-through;
    padding-bottom: 6rpx;
    margin-left: 20rpx;
}

.goods-info .brand{
    min-height: 40rpx;
    text-align: center;
}

.goods-info .brand text{
    display: inline-block;
    width: auto;
    padding: 4px 24rpx 4px 18rpx;
    border: 1px solid #f48f18;
    font-size: 20rpx;
    color: #f48f18;
    border-radius: 4rpx;
    background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/detailTagArrow-18bee52dab.png) 92% center no-repeat;
    background-size: 8rpx 16rpx;
}

.section-nav{
    height: 108rpx;
    background: #fff;
    margin-bottom: 20rpx;
    display:flex;
    align-items: center;    
}

.section-nav .t{
    flex: 1;
    float: left;
    height: 108rpx;
    line-height: 108rpx;
    font-size: 29rpx;
    color: #333;
}

.section-nav .i{
    float: right;
    width: 40rpx;
    height: 40rpx;
}

.section-act .t{
    float: left;
    display: flex;
    align-items: center;
    width: 600rpx;
    height: 108rpx;
    overflow: hidden;
    line-height: 108rpx;
    font-size: 29rpx;
    color: #999;
    margin-left: 31.25rpx;
}

.section-act .label{
    color: #999;
}

.section-act .tag{
    display: flex;
    align-items: center;
    padding:0 10rpx;
    border-radius: 3px;
    height: 37rpx;
    width: auto;
    color: #f48f18;
    overflow: hidden;
    border: 1px solid #f48f18;
    font-size: 25rpx;
    margin:0 10rpx;
}

.section-act .text{
    display: flex;
    align-items: center;
    height: 37rpx;
    width: auto;
    overflow: hidden;
    color: #f48f18;
    font-size: 29rpx;
}

